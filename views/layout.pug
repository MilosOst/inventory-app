doctype html
html(lang='en')
head
	title= title
	meta(charset='utf-8')
	meta(name='viewport', content='width=device-width, initial-scale=1')
	link(rel='stylesheet', href='/stylesheets/style.css')
	<link rel="preconnect" href="https://fonts.googleapis.com">
	<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
	<link href="https://fonts.googleapis.com/css2?family=Roboto:wght@100&display=swap" rel="stylesheet">
body
	nav.navbar 
		h2.logo Logo
		ul.main-links 
			li #[a(href='/' class='nav-link') Home] 
			li #[a(href='/brands' class='nav-link') Brands]
			li #[a(href='/listings' class='nav-link') Listings]
		ul.account-links 
			li #[a(href='/login' class='nav-link btn nav-btn') Log In] 
			li #[a(href='/signup' class='nav-link btn nav-btn') Sign Up]
	.content
		block content
